    <% if locals[:sala] == 2 %>
      <script>
        $(function() {
          $(".tab-content").fadeOut();
          $("#sala_2").fadeIn();
          $("ul#tabs li a").removeClass("active");
          $("ul#tabs li a#nav_sala_2").addClass("active");
        });
      </script>
    <% end %>
    
    <!--[if lte IE 8]>
