  <body>
    <header>
      <div id='logo'>
        <a href='/'>Dev in Rio 2011</a>
      </div>

      <div id='info'>
        <div id="when">Sábado - 24 de setembro</div>
        <div id="where"><a href="/localizacao">Hotel Guanabara - Centro - Rio de Janeiro</a></div>
      </div>
    </header>
    <hr />
    <div id="container">
      <section role='main'>
        <%= yield %>
      </section>
      <hr />
      <aside>
        <nav id="social">
          <ul>
            <li><a href="http://twitter.com/devinrio" id="twitter" rel="external">Twitter</a></li>
            <li><a href="http://facebook.com/devinrio" id="facebook" rel="external">Facebook</a></li>
            <li><a href="mailto:contato@devinrio.com.br" id="email">contato@devinrio.com.br</a></li>
            <li><a href="http://vimeo.com/devinrio" id="vimeo" rel="external">Vimeo</a></li>
          </ul>
        </nav>
        <nav id="menu">
          <ul>
            <li id="suggest"><a href="https://spreadsheets.google.com/spreadsheet/viewform?formkey=dEVIakJQa004RVdENnNMZWs5ZEtWNmc6MQ" rel="external">Sugira um assunto!</a></li>
            <li><a href="/2009">Evento 2009</a></li>
            <li><a href="/2010">Evento 2010</a></li>
            <li><a href="/files/Dev-in-Rio-2011-plano-de-captacao.pdf">Plano de captação</a></li>
          </ul>
        </nav>
        
        <a class='twitter-share-button' data-count='horizontal' data-via='devinrio' href='http://twitter.com/share'>Tweet</a>
        <br />
        <iframe src="http://www.facebook.com/plugins/like.php?href=http://www.facebook.com/devinrio&amp;layout=button_count&amp;show_faces=false&amp;width=200&amp;action=like&amp;font=arial&amp;colorscheme=light&amp;height=20&amp;locale=pt_BR" frameborder="0" allowTransparency="true" scrolling="no" style="border:none; overflow:hidden; width:200px; height:20px; margin: 10px 0;"></iframe>
        <br />
        <g:plusone href='http://www.devinrio.com.br' size='medium'></g:plusone>

      </aside>
    </div>
    <hr />
    <footer>
      <div id="footer-inner">
        <div id="sponsors">
          <h2>Patrocínio</h2>
      
          <nav>
            <ul>
              <li><a href="http://globo.com" rel="external"><img src="/images/logos/globo-com.png" alt="Globo.com" /></a></li>
              <li><a href="http://www.gonow.com.br" rel="external"><img src="/images/logos/gonow.png" alt="Gonow" /></a></li>
            </ul>
          </nav>
